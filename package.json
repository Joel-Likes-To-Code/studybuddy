{
  "name": "studybuddy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "db:logs": "docker compose logs -f db",
    "// Prisma - local dev against Docker": "",
    "prisma:generate": "dotenv -e .env.docker -- prisma generate",
    "prisma:migrate:dev": "dotenv -e .env.docker -- prisma migrate dev",
    "prisma:studio": "dotenv -e .env.docker -- prisma studio",
    "// Prisma - apply migrations on Supabase": "",
    "prisma:migrate:deploy": "dotenv -e .env.supabase -- prisma migrate deploy",
    "prisma:reset:supabase": "dotenv -e .env.supabase -- prisma migrate reset --force"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@auth/supabase-adapter": "^1.10.0",
    "@prisma/client": "^6.16.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/supabase-js": "^2.57.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.543.0",
    "next": "15.5.3",
    "next-auth": "5.0.0-beta.29",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "prisma": "^6.16.0",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5"
  }
}
